<Window x:Class="project.ConcreteSpec"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="450" Width="800">
    <Grid>
        <ListView ItemsSource="{Binding Workers}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="140" Header="FirstName" DisplayMemberBinding="{Binding FirsnName }"  />
                    <GridViewColumn Width="140" Header="LastName" DisplayMemberBinding="{Binding Lastlame }" />
                    <GridViewColumn Width="140" Header="Surname" DisplayMemberBinding="{Binding Surname }" />
                    <GridViewColumn Width="140" Header="Gender" DisplayMemberBinding="{Binding Gender}" />
                    <GridViewColumn Width="140" Header="Phone" DisplayMemberBinding="{Binding Phone}" />
                    <GridViewColumn Width="240" Header="Birthdate" DisplayMemberBinding="{Binding BirthDate }" />
                    <GridViewColumn Width="140" Header="Speciality" DisplayMemberBinding="{Binding Specialties.SpecName }" />
                    <GridViewColumn Width="140" Header="Salary" DisplayMemberBinding="{Binding Salary }" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
